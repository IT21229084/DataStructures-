		   // Create a new queue of characters in ascending order
        Queue<Character> q1 = new LinkedList<Character>();
        q1.add('A');
        q1.add('B');
        q1.add('C');
        q1.add('D');
        q1.add('E');
        
        // Print out the original queue
        System.out.println("Original queue in ascending order: " + q1);
        
        // Create a new stack to hold the characters temporarily
        Stack<Character> s1 = new Stack<Character>();
        
        // Pop each character from the queue and push it onto the stack
        while (!q1.isEmpty()) {
            s1.push(q1.remove());
        }
        
        // Pop each character from the stack and add it back to the queue
        while (!s1.isEmpty()) {
            q1.add(s1.pop());
        }
        
        // Print out the new queue in descending order
        System.out.println("New queue in descending order: " + q1);
    }

		
		
		
		
		
	}


