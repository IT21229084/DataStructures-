		 queue thecquque = new queue(5);

         stack thestack = new stack(5);

         Scanner forcqueue = new Scanner(System.in);   

         System.out.print("Enter the character : ");

         String d = forcqueue.next();

        

         for(int i=0; i<5; i++) {

         thecquque.insert(d.charAt(i));

         }

         System.out.println("Ascending order ");

         while(!thecquque.isEmpty()) {

                         int r = thecquque.remove();

                         System.out.println(r);

                         thestack.push(r);

         }

         System.out.println("");

         System.out.println("Disending order ");

         while(!thestack.isEmpty()) {

                         char t = thestack.pop();

                         thecquque.insert(t);

                         System.out.println(t);   

         }

         System.out.println("");

         System.out.println("After store to the queue disending order ");

         while(!thecquque.isEmpty()) {

                         int t = thecquque.remove();

                         System.out.println(t);                   

         }                                             

		
		
		
		
