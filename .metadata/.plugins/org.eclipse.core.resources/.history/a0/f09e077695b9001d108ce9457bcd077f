package test;

import java.util.Scanner;

public class main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		queue q = new queue(5);
		stack st = new stack(5);
		
		for (int i =0;i<5;i++) {
			System.out.println("Enter value : ");
			int n = sc.nextInt();
			q.insert(n);
			//st.push(n);
			
		}
		while(!q.isEmpty()) {
			int a = q.remove();
			st.push(a);
		}
		
		while(!st.isEmpty()) {
			int x = st.pop();
			q.insert(x);
		}
		
		while(!q.isEmpty()) {
			System.out.println("Value = "+q.remove());
			
		}
		
		
		
		
		
	}

}
