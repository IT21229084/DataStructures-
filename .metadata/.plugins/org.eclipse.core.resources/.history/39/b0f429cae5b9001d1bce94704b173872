		stack s=new stack(5);//create stack object
		queue q=new queue(5);//create queue object
		
		Scanner sc1 =new Scanner(System.in);
		
	
		System.out.println("Before");
		System.out.println();
		
		//input keyboard values in ascending order
		for(int i=0;i<5;i++) {
			
		    char value=sc1.next().charAt(0);//read keyboard inputs and assign them to value variable
			q.insert(value);//insert into queue
		}
		
		//remove values from the queue and push them to stack
		while(!q.isEmpty()) {
			char value=q.remove();
			s.push(value);
			
		}
		
	    System.out.println();
		System.out.println("After"); 
		System.out.println();
		
		//remove values from the stack and insert them to queue in descending order
		while(!s.isEmpty()) {
			char val=s.pop();//assign pop values to val variable
			q.insert(val);

		}
		
		//remove values from the queue
		while(!q.isEmpty())
		{
			System.out.print(q.remove()+" ");
		}
	
		
		
		
		
	}

