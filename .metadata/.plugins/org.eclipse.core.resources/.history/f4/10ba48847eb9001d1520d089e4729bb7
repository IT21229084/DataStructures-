package q02;

import java.util.Scanner;

public class main {

	public static void main(String[] args) {
		
		queue qu = new queue (10);
		stack st = new stack(5);
		Scanner sc = new Scanner(System.in);
		
		for (int i=0;i<5;i++) {
			System.out.print("Enter value");
			int n = sc.nextInt();
			qu.insert(n);	
		}
		
		while(!qu.isEmpty()) {
			st.push(qu.remove());
		}
		
		while(!st.isEmpty()) {
			qu.insert(st.pop());
		}
		
		while(!qu.isEmpty()) {
			System.out.print("Value: "+qu.remove());
		}
		
		
		
		

	}

}
