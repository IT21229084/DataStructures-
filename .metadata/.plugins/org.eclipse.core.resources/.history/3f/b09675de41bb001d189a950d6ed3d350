package q02;

import java.util.Scanner;

public class main {

	public static void main(String[] args) {
		
		 Queue que = new Queue(10);
	        que.insert("1231");
	        que.insert("1221");
	        que.insert("4562");
	        que.insert("7893");
	        que.insert("1013");
	        que.insert("1122");
	        que.insert("1323");
	        
	        Stack pump1 = new Stack(5);
	        Stack pump2 = new Stack(5);
	        Stack pump3 = new Stack(5);
	        
	        while (!que.isEmpty()) {
	        	String vehicleID = que.remove();
	           // String ID = Integer.toString(vehicleID);
	        	char lastDigit = vehicleID.charAt(3) ;
	            switch (lastDigit) {
	                case '1':
	                    pump1.push(vehicleID);
	                    break;
	                case '2':
	                    pump2.push(vehicleID);
	                    break;
	                case '3':
	                    pump3.push(vehicleID);
	                    break;
	                default:
	                    System.out.println("Invalid vehicle ID: " + vehicleID);
	                    break;
	            }
	        }
	        
	        System.out.println("pump 1:");
	        while (!pump1.isEmpty()) {
	            System.out.println(pump1.pop());
	        }
	        
	        System.out.println("pump 2:");
	        while (!pump2.isEmpty()) {
	            System.out.println(pump2.pop());
	        }
	        
	        System.out.println("pump 3:");
	        while (!pump3.isEmpty()) {
	            System.out.println(pump3.pop());
	        }
	    }
		
		

	}


