package linkList;

public class LinkList {
	
	private Link First;

	public LinkList() {
		
		First = null;
	}
	
	public void DisplayLinkList() {
		
		Link Current = First;
		
		while(Current != null) {
			
			Current.DisplayLink();
			Current = Current.Next;
			
		}
		System.out.println(" \n");
	}
	
	public void InsertFirst(int Data) {
		
		Link newLink = new Link(Data);
		newLink.Next = First;
		First = newLink;
				
	}
	
	public Link DeleteFirst() {
		Link Temp = First;
		First = First.Next;	
		return Temp;
	}
	
	
	
	

}
